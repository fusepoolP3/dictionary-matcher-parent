@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix ex: <http://www.products.com/>.
@prefix owl: <http://www.w3.org/2002/07/owl#>.

ex:Product rdf:type owl:Class;
  a skos:Concept;
  skos:prefLabel "Product"@en.

ex:Camera rdf:type owl:Class;
  a skos:Concept;
  skos:prefLabel "Camera"@en;
  skos:altLabel "Digital Camera"@en;
  skos:altLabel "Digicam"@en;
  skos:altLabel "Digital SLR"@en;
  skos:altLabel "Disposable Camera"@en;
  skos:altLabel "Film Camera"@en;
  skos:altLabel "Instant Camera"@en;
  skos:altLabel "Video Camera"@en;
  skos:altLabel "Web Camera"@en;
  skos:altLabel "Webcam"@en;
  skos:broader ex:Product. 
  
ex:Television rdf:type owl:Class;
  a skos:Concept;
  skos:prefLabel "Television"@en;
  skos:altLabel "LCD TV"@en;
  skos:altLabel "LCD Television"@en;
  skos:altLabel "Plasma Television"@en;
  skos:altLabel "Plasma TV"@en;
  skos:altLabel "3D Television"@en;
  skos:altLabel "3D TV"@en;
  skos:altLabel "LED Television"@en;
  skos:altLabel "LED TV"@en;
  skos:altLabel "CRT Television"@en;
  skos:altLabel "Portable Television"@en;
  skos:broader ex:Product. 

ex:Notebook rdf:type owl:Class;
  a skos:Concept;
  skos:prefLabel "Notebook"@en;
  skos:altLabel "Notebok"@en;
  skos:altLabel "Laptop"@en;
  skos:broader ex:Product. 
  
ex:Mobile rdf:type owl:Class;
  a skos:Concept;
  skos:prefLabel "Mobile"@en;
  skos:altLabel "Mobile Phone"@en;
  skos:altLabel "Cellphone"@en;
  skos:altLabel "Cell Phone"@en;
  skos:altLabel "Mobile Telephone"@en;
  skos:altLabel "Hand Phone"@en;
  skos:broader ex:Product. 
  
ex:Jewellery rdf:type owl:Class;
  a skos:Concept;
  skos:prefLabel "Jewellery"@en;
  skos:altLabel "Jewelry"@en;
  skos:altLabel "Jewelery"@en;
  skos:broader ex:Product. 